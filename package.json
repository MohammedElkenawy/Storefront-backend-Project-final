{
  "name": "storefront-backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc && node -r dotenv/config ./dist/app.js",
    "dev": "db-migrate up && ts-node-dev --respawn --cls --rs -r dotenv/config -- ./src/app.ts",
    "lint": "eslint --ext .js,.ts ./src --config .eslintrc.json --fix",
    "format": "prettier --config .prettierrc.json --write \"./**/*.ts\"",
    "migrate-up": "db-migrate up",
    "migrate-down": "db-migrate reset",
    "test": "set NODE_ENV=TEST && db-migrate up --env test && tsc && jasmine --config=\"spec/support/jasmine.json\" && db-migrate reset && set NODE_ENV=DEVELOPMENT"
  },
  
  "keywords": [],
  "author": "MohamedElkenawy <mohammedelkenawy1987@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/db-migrate-pg": "^0.0.10",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.10.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.18",
    "@types/pg": "^8.6.4",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine": "^4.0.0",
    "jasmine-spec-reporter": "^7.0.0",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2",
    "ts-node": "^10.6.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  }
 